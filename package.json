{
	"name": "bug-tracker",
	"version": "1.0.0",
	"description": "A bug tracking application",
	"main": "server.js",
	"scripts": {
		"start": "node server/server",
		"server": "nodemon server/server",
		"mockServer": "nodemon server/mockServer",
		"test": "cross-env NODE_ENV=test jest --forceExit --detectOpenHandles --passWithNoTests && cross-env NODE_ENV=test yarn --cwd client test",
		"client": "yarn --cwd client start",
		"dev": "concurrently \"npm run client\" \"npm run mockServer\"",
		"db:import": "node server/data/generateData",
		"db:destroy": "node server/data/generateData -d",
		"heroku-postbuild": "NPM_CONFIG_PRODUCTION=false yarn --cwd client && yarn --cwd client build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/scottduller/bug-tracker.git"
	},
	"author": "Scott Duller",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/scottduller/bug-tracker/issues"
	},
	"homepage": "https://github.com/scottduller/bug-tracker#readme",
	"dependencies": {
		"babel-plugin-macros": "^3.1.0",
		"colors": "^1.4.0",
		"cors": "^2.8.5",
		"cross-env": "^7.0.3",
		"eslint": "7.20.0",
		"eslint-plugin-react": "7.22.0",
		"express": "^4.17.1",
		"jest": "26.6.0",
		"jsonwebtoken": "^8.5.1",
		"uuid": "^8.3.2"
	},
	"devDependencies": {
		"@babel/cli": "^7.13.10",
		"@babel/core": "^7.13.10",
		"@babel/node": "^7.13.12",
		"@babel/preset-env": "^7.13.12",
		"@babel/preset-react": "^7.12.13",
		"concurrently": "^6.2.1",
		"faker": "^5.5.3",
		"json-server": "^0.16.3",
		"nodemon": "^2.0.12"
	}
}
